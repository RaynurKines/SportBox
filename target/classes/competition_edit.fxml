<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="450.0" maxWidth="400.0" minHeight="450.0" minWidth="400.0" prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="450.0" prefWidth="400.0">
         <children>
            <MenuBar fx:id="Menu" minHeight="30.0" minWidth="80.0" prefHeight="26.0" prefWidth="700.0">
               <menus>
                  <Menu fx:id="Menu_event" mnemonicParsing="false" text="События" />
                  <Menu fx:id="Menu_student" mnemonicParsing="false" text="Студенты" />
               </menus>
            </MenuBar>
            <HBox prefHeight="36.0" prefWidth="600.0" spacing="10.0">
               <children>
                  <Button fx:id="backButton" maxHeight="30.0" maxWidth="85.0" minHeight="30.0" minWidth="40.0" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="30.0" prefWidth="85.0" text="Назад" />
                  <Button fx:id="saveButton" maxHeight="30.0" maxWidth="85.0" minHeight="30.0" minWidth="40.0" mnemonicParsing="false" onAction="#saveButtonAction" prefHeight="30.0" prefWidth="85.0" text="Сохранить">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <VBox prefHeight="200.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <TextField fx:id="name_competition" alignment="CENTER" text="Название состязания" />
                  <TextField fx:id="event" alignment="CENTER" text="Событие" />
                  <TextField fx:id="student" alignment="CENTER" text="Студент" />
                  <TextField fx:id="place" alignment="CENTER" prefHeight="26.0" prefWidth="376.0" text="Место" />
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
      <ImageView fx:id="ImageView" fitHeight="150.0" fitWidth="200.0" layoutX="100.0" layoutY="270.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
